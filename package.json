{
  "name": "@xikww/mocket.io-client",
  "version": "0.1.0",
  "description": "Mock Socket.IO client connections for reliable integration testing with Vitest",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vitest-setup.cjs",
  "module": "./dist/vitest-setup.js",
  "types": "./dist/vitest-setup.d.ts",
  "exports": {
    "./setup": {
      "import": {
        "types": "./dist/vitest-setup.d.ts",
        "default": "./dist/vitest-setup.js"
      },
      "require": {
        "types": "./dist/vitest-setup.d.ts",
        "default": "./dist/vitest-setup.cjs"
      }
    },
    "./context": {
      "import": {
        "types": "./dist/vitest-context.d.ts",
        "default": "./dist/vitest-context.js"
      },
      "require": {
        "types": "./dist/vitest-context.d.ts",
        "default": "./dist/vitest-context.cjs"
      }
    }
  },
  "scripts": {
    "test:lib": "vitest run --dir src",
    "test:examples": "vitest run examples/*.test.ts",
    "build": "vite build",
    "print-paths": "node -e 'console.log(module.paths)'",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "keywords": [
    "socket.io",
    "socket.io-client",
    "mock",
    "mocking",
    "testing",
    "vitest",
    "test",
    "unit-test",
    "websocket"
  ],
  "author": {
    "name": "Francisco Cunha",
    "email": "your-email@example.com",
    "url": "https://your-website.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.10",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vite": "^4.0.0"
  },
  "peerDependencies": {
    "socket.io-client": "^4.8.1",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "vite-plugin-dts": "^4.5.3"
  }
}
